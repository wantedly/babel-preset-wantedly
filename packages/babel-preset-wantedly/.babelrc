{
  "presets": [
    [
      "@babel/env",
      {
        "targets": { "node": "current" },
        "modules": "commonjs"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-object-rest-spread"],
  "sourceMaps": true,
  "comments": false
}
